javascript:(function(){try{var s=document.createElement('style');s.textContent='textarea,input,.MuiInputBase-input,[contenteditable="true"]{user-select:text !important;-webkit-user-select:text !important;-ms-user-select:text !important;pointer-events:auto !important;opacity:1 !important;cursor:text !important}button,a,input[type="button"],input[type="submit"]{pointer-events:auto !important;opacity:1 !important;cursor:pointer !important}';document.head.appendChild(s);var fieldsSel=['textarea','input[type="text"]','input[type="search"]','input[type="email"]','input[type="password"]','input[type="url"]','input[type="number"]','input[type="tel"]','.MuiInputBase-input','[contenteditable="true"]'].join(','),scrub=function(el){try{if(!el.matches('input, textarea, .MuiInputBase-input, [contenteditable="true"]'))return;el.removeAttribute('readonly');el.removeAttribute('disabled');['oncopy','oncut','onpaste','oncontextmenu','onkeydown','onkeyup','onselectstart','ondragstart'].forEach(function(a){el.removeAttribute(a);try{el[a]=null}catch(e){}});el.style.userSelect='text';el.style.webkitUserSelect='text';el.style.msUserSelect='text';el.style.pointerEvents='auto';el.style.opacity='1'}catch(_){}};document.querySelectorAll(fieldsSel).forEach(scrub);var mo=new MutationObserver(function(m){m.forEach(function(mu){mu.addedNodes&&mu.addedNodes.forEach(function(n){if(!(n instanceof Element))return;if(n.matches&&n.matches(fieldsSel))scrub(n);n.querySelectorAll&&n.querySelectorAll(fieldsSel).forEach(scrub)})})});mo.observe(document.documentElement,{childList:true,subtree:true});var isField=function(t){return t instanceof Element&&!!t.closest(fieldsSel)},stop=function(e){if(!isField(e.target))return;e.stopImmediatePropagation()},kd=function(e){if(!isField(e.target))return;var k=(e.key||'').toLowerCase(),combo=(e.ctrlKey||e.metaKey),isCb=(combo&&['c','x','v','insert'].includes(k))||(e.shiftKey&&k==='insert');if(isCb)e.stopImmediatePropagation()};['copy','cut','paste','contextmenu','selects